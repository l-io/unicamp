#include "MKL25Z4.h"
#include "derivative.h" /* include peripheral declarations */
#include "delay.h"
#include "lcd.h"


int main(void)
{
	int counter = 0;
	
	init_GPIO_LCD();
	init_LCD();
	
	putchar_LCD('B');
	putchar_LCD('L');
	putchar_LCD('o');
	putchar_LCD(' ');
	putchar_LCD('M');
	putchar_LCD('U');
	putchar_LCD('N');
	putchar_LCD('D');
	putchar_LCD('O');
	
	for(counter; counter < 50000; counter++) {	   
	   	counter++;
	}
	
	clear_LCD();
	for(;;) {	   
		   	counter++;
	}
	return 0;
}
